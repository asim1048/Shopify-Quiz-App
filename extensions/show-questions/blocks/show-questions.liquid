<div class="app-form">
    <h1 class="text-align:{{ block.settings.alignment }}">{{ block.settings.form_title }}</h1>
   
    
        <div class="quiz-container">
            <h2>{{ quiz.title }}</h2>
            {% for question in questions %}
                <div class="question">
                    <h3>{{ question.title }}</h3>
                    
                </div>
            {% endfor %}
        </div>
    
</div>



   {% schema %}
       {
          "name": "Showing Questions",
          "target": "section",
          "class": "app-dashboard-app",
          "stylesheet": "questions.css",
          "javascript": "questions.js",
          "settings": [
           {
              "label": "App Form Title",
              "id": "form_title",
              "type": "text"
           },
           {
              "type": "text",
              "id": "alignment",
              "label": "Text Alignment",
              "default": "center"
           }
          ]
       }
       {% endschema %}